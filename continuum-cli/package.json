{
  "name": "continuum-cli",
  "version": "0.1.0",
  "description": "Continuum Framework CLI",
  "author": "Navid Mitchell @NavidMitchell",
  "bin": {
    "continuum": "./bin/run"
  },
  "homepage": "https://github.com/Kinotic-Foundation/continuum-framework",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "Kinotic-Foundation/continuum-framework",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-autocomplete": "^1.2.0",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-not-found": "^2.3.1",
    "@oclif/plugin-plugins": "^2.0.1",
    "@oclif/plugin-update": "^3.0.0",
    "@oclif/plugin-warn-if-update-available": "^2.0.4",
    "inquirer": "^8.2.0",
    "liquidjs": "^9.34.1",
    "make-dir": "^3.1.0"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/inquirer": "^8.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.19",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^2",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "continuum",
    "dirname": "continuum",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-update",
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-autocomplete"
    ],
    "topicSeparator": " ",
    "topics": {
      "create": {
        "description": "Creates Continuum projects, applications, and services"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/Kinotic-Foundation/continuum-framework/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
